{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "buildCommand": "./vercel-build.sh",
  "installCommand": "npm run setup",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/articles", "dest": "/articles.html" },
    { "src": "/sheets", "dest": "/sheets.html" },
    { "src": "/search", "dest": "/search.html" },
    { "src": "/(.*)", "dest": "/error.html" }
  ]
}